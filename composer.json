{
    "name"       : "kheme/php-supertext-nigeria",
    "description": "A simple PHP wrapper for SuperText Nigeria's SMS gateway API",
    "version"    : "1.0.0",
    "keywords"   : [
        "kheme",
        "php",
        "sms",
        "php sms package",
        "supertextng",
        "sms gateway",
        "php sms gateway",
        "php sms sender"
    ],
    "homepage": "https://github.com/kheme/php-supertext-nigeria",
    "type"    : "package",
    "license" : "MIT",
    "authors" : [
        {
            "name"    : "Okiemute Omuta",
            "email"   : "iamkheme@gmail.com",
            "homepage": "https://github.com/kheme",
            "role"    : "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Kheme\\SuperTextNg\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable"    : true,
    "require"          : {
        "php": ">=5.3.0"
    },
    "repositories": {
        "package-name": {
            "type"   : "path",
            "url"    : "packages/kheme/laravel-supertext-nigeria",
            "options": {
                "symlink": true
            }
        }
    }
}
